skos:prefLabel
  a owl:ObjectProperty ;
  rdfs:label "preferred label"@en,
    "priporočena oznaka"@sl ;
  skos:definition "The preferred labels are useful when generating or creating human-readable representations of a knowledge organization system. These labels provide the strongest clues as to the meaning of a SKOS concept."@en,
    "Priporočene oznake so uporabne, ko se generira ali izdeluje človeku razumljive predstavitve organizacij znanja. Te oznake zagotavljajo najbolj smiseln opis pomena konceptov SKOS."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  rdfs:domain skos:Concept, dqv:Dimension ;
  rdfs:range rdfs:Literal .

foaf:primaryTopic
  a owl:ObjectProperty ;
  rdfs:label "primary topic"@en,
    "primarna tematika"@sl ;
  skos:definition "The primary topic of some page or document. "@en,
    "Primarna tematika neke strani ali dokumenta."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  owl:maxCardinality 1 ;
  rdfs:domain dcat:CatalogRecord ;
  rdfs:range dcat:Dataset .

dcat:endpointURL
  a owl:ObjectProperty ;
  rdfs:label "service endpoint"@en,
    "naslov storitve"@sl ;
  skos:definition "The root location or primary endpoint of the service (a web-resolvable IRI)."@en,
    "Korenska lokacija ali primarni naslov storitve (spletno dostopen IRI)."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  rdfs:domain dcat:DataService ;
  rdfs:range dcat:Dataset .

dcat:endpointDescription
  a owl:ObjectProperty ;
  rdfs:label "description of service end-point"@en,
    "opis naslova storitve"@sl ;
  skos:definition "A description of the service end-point, including its operations, parameters etc."@en,
    "Opis naslova storitve, vključno z njenimi operacijami, parametri, ipd."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain dcat:DataService ;
  rdfs:range dcat:Resource .

dcat:servesDataset
  a owl:ObjectProperty ;
  rdfs:label "serves dataset"@en,
    "ponuja podatkovni vir"@sl ;
  skos:definition "A collection of data that this DataService can distribute."@en,
    "Zbirka podatkov, ki jih opisana podatkovna storitev lahko distribuira."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain dcat:DataService ;
  rdfs:range dcat:Dataset .

pz:legalNoSOP
  a owl:ObjectProperty ;
  rdfs:label "legal ground non-SOP"@en,
    "pravna podlaga brez SOP"@sl ;
  skos:definition "Legal ground without having a SOP number."@en,
    "Pravna podlaga brez številke SOP."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain pz:LegalGround ;
  rdfs:range rdfs:Literal .

pz:legalSOP
  a owl:ObjectProperty ;
  rdfs:label "legal ground SOP"@en,
    "pravna podlaga SOP"@sl ;
  skos:definition "Legal ground having a SOP number."@en,
    "Pravna podlaga s številko SOP."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain pz:LegalGround ;
  rdfs:range rdfs:Literal .

dqv:value
  a owl:ObjectProperty ;
  rdfs:label "evaluation value"@en,
    "evalvacijska vrednost"@sl ;
  skos:definition "Refers to values computed by metric."@en,
    "Nanašanje na vrednosti, ki so bile izračunanje glede na metriko."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  owl:maxCardinality 1 ;
  rdfs:domain dqv:QualityMeasurement ;
  rdfs:range rdfs:Literal .

dqv:computedOn
  a owl:ObjectProperty ;
  rdfs:label "computed on"@en,
    "izračunano na"@sl ;
  skos:definition "Refers to the resource (e.g., a dataset, a linkset, a graph, a set of triples) on which the quality measurement is performed. In the DQV context, this property is generally expected to be used in statements in which objects are instances of dcat:Dataset or dcat:Distribution."@en,
    "Nanašanje na vir (npr. podatkovni vir, povezan seznam podatkov, graf ali seznam trojic), na katerij se izvaja merjenje kvalitete. V kontekstu DQV je ta lastnost načeloma uporabljena nad primerki dcat:Dataset ali dcat:Distribution."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  owl:maxCardinality 1 ;
  rdfs:domain dqv:QualityMeasurement ;
  rdfs:range rdfs:Resource .

dqv:isMeasurementOf
  a owl:ObjectProperty ;
  rdfs:label "measurement of"@en,
    "meritev"@sl ;
  skos:definition "Indicates the metric being observed."@en,
    "Označuje opazovano metriko."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  owl:maxCardinality 1 ;
  rdfs:domain dqv:QualityMeasurement ;
  rdfs:range dqv:Metric .

