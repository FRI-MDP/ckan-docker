  rdfs:label "access rights"@en ;
  rdfs:label "pravice dostopa"@sl ;
  skos:definition "Information about who access the resource or an indication of its security status."@en,
    "Podatki o dostopih do vira in indikator statusa varnosti."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain dcat:Dataset, dcat:DataService ;
  rdfs:range dct:RightsStatement .

dct:source
  a owl:ObjectProperty ;
  rdfs:label "source"@en ;
  rdfs:label "vir"@sl ;
  skos:definition "A related resource from which the described resource is derived."@en,
    "Povezan vir, iz katerega je bil opisan vir izpeljan."@sl ;
  vaem:rationale "Optional. Domain and range types should match."@en,
    "Opcijsko. Domain and range types should match."@sl ;
  rdfs:domain dcat:Dataset, dcat:CatalogRecord ;
  rdfs:range dcat:Dataset, dcat:CatalogRecord .

adms:sample
  a owl:ObjectProperty ;
  rdfs:label "sample"@en,
    "vzorec"@sl ;
  skos:definition "Links to a sample of an Asset (which is itself an Asset)."@en,
    "Povezava na vzorec vira (ki je hkrati tudi sam po sebi vir)."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain dcat:Dataset ;
  rdfs:range dcat:Dataset .

dct:hasVersion
  a owl:ObjectProperty ;
  rdfs:label "has version"@en,
    "ima verzijo"@sl ;
  skos:definition "A related resource that is a version, edition, or adaptation of the described resource."@en,
    "Povezan vir, ki je verzija, izdaja ali prilagoditev opisanega vira."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain dcat:Dataset ;
  rdfs:range dcat:Dataset .

dct:isVersionOf
  a owl:ObjectProperty ;
  rdfs:label "is version of"@en,
    "je verzija"@sl ;
  skos:definition "A related resource of which the described resource is a version, edition, or adaptation."@en,
    "Povezan vir, ki je verzija, izdaja ali prilagoditev opisanega vira."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain dcat:Dataset ;
  rdfs:range dcat:Dataset .

dqv:hasQualityMeasurement
  a owl:ObjectProperty ;
  rdfs:label "has quality measurement"@en,
    "ima mero kakovosti"@sl ;
  skos:definition "Refers to the performed quality measurements. Quality measurements can be performed to any kind of resource (e.g., a dataset, a linkset, a graph, a set of triples). However, in the DQV context, this property is generally expected to be used in statements in which subjects are instances of dcat:Dataset or dcat:Distribution."@en,
    "Lastnost se nanaša na izvedeno merjenje kvalitete. Merjenje kvalitete se lahko meri za katerikoli vir (npr. podatkovni vir, povezan seznam, graf, seznam trojic). Kljub temu je v okviru DQV ta lastnost načeloma pričakovana za uporabo, ko so subjekti primerki konceptov dcat:Dataset ali dcat:Distribution."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain dcat:Dataset ;
  rdfs:range dqv:QualityMeasurement .

dcat:bbox
  a owl:ObjectProperty ;
  rdfs:label "bounding box"@en,
    "omejeno področje"@sl ;
  skos:definition "The geographic bounding box of a resource."@en,
    "Geografsko omejeno področje vira."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  owl:maxCardinality 1 ;
  rdfs:domain dct:location ;
  rdfs:range rdfs:Literal .

dcat:centroid
  a owl:ObjectProperty ;
  rdfs:label "centroid"@en,
    "centroid"@sl ;
  skos:definition "The geographic center (centroid) of a resource."@en,
    "Geografski center (centroid) vira."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  owl:maxCardinality 1 ;
  rdfs:domain dct:Location ;
  rdfs:range rdfs:Literal .

locn:geometry
  a owl:ObjectProperty ;
  rdfs:label "geometry"@en,
    "geometrija"@sl ;
  skos:definition "Associates any Resource with the corresponding Geometry."@en,
    "Povezuje katerikoli vir s pripadajočo geometrijo."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  owl:maxCardinality 1 ;
  rdfs:domain dct:Location ;
  rdfs:range rdfs:Literal .

skos:notation
  a owl:ObjectProperty ;
  rdfs:label "notation"@en,
    "notacija"@sl ;
  skos:definition "A notation is different from a lexical label in that a notation is not normally recognizable as a word or sequence of words in any natural language."@en,
    "Notacija je drugačna od leksikalne oznake na način, da notacija ni splošno prepoznana kot beseda ali zaporedje besed v kateremkoli od naravnih jezikov."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:maxCardinality 1 ;
  owl:minCardinality 1 ;
  rdfs:domain adms:Identifier ;
  rdfs:range rdfs:Resource .

skos:inScheme
  a owl:ObjectProperty ;
  rdfs:label "is in scheme"@en,
    "je v shemi"@sl ;
  skos:definition "Relates a resource (for example a concept) to a concept scheme in which it is included."@en,
    "Povezuje vir (npr. koncept) s shemo konceptov, v kateri je vsebovan."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:maxCardinality 1 ;
  owl:minCardinality 1 ;
  rdfs:domain pz:Object ;
  rdfs:range csvw:Table .

pz:inDistribution
  a owl:ObjectProperty ;
  rdfs:label "is part of distribution"@en,
    "je del distribucije"@sl ;
  skos:definition "Relates an instance to a distribution."@en,
    "Povezuje primerek z distribucijo."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:maxCardinality 1 ;
  owl:minCardinality 1 ;
  rdfs:domain pz:Object ;
  rdfs:range dcat:Distribution .

pz:schema
  a owl:ObjectProperty ;
  rdfs:label "uses scheme"@en,
    "uporablja shemo"@sl ;
  skos:definition "Defines a schema that is used by a distribution."@en,
    "Definira shemo, ki jo uporablja specifična distribucija."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:maxCardinality 1 ;
  owl:minCardinality 1 ;
  rdfs:domain dcat:Distribution ;
  rdfs:range csvw:Table .

