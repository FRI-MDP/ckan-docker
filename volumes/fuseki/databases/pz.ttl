dqv:inDimension
  a owl:ObjectProperty ;
  rdfs:label "in dimension"@en,
    "v dimenziji"@sl ;
  skos:definition "Represents the dimensions a quality metric, certificate and annotation allow a measurement of."@en,
    "Predstavlja dimenzijo, ki jih kvalitativna metrika, certifikat in oznaka dovoljujejo izmeriti."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  owl:maxCardinality 1 ;
  rdfs:domain dqv:Metric ;
  rdfs:range dqv:Dimension .

dqv:expectedDataType
  a owl:ObjectProperty ;
  rdfs:label "expected data type"@en,
    "pričakovan podatkovni tip"@sl ;
  skos:definition "Represents the expected data type for the metric's observed value (e.g., xsd:boolean, xsd:double etc...)"@en,
    "Predstavlja pričakovan podatkovni tip za opazovano vrednost metrike (npr. xsd:boolean, xsd:double, ipd.)."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain dqv:Metric ;
  rdfs:range rdfs:Datatype .

prov:wasDerivedFrom
  a owl:ObjectProperty ;
  rdfs:label "was derived from"@en,
    "pričakovan podatkovni tip"@sl ;
  skos:definition "A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity."@en,
    "Izpeljava je transformacija entitete v drugo, posodobitev entitete, s čimer se izdela nova, ali konstrukcija nove entitete na podlagi obstoječe."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain dqv:Metric ;
  rdfs:range dct:Standard .

dqv:inCategory
  a owl:ObjectProperty ;
  rdfs:label "in category"@en,
    "v kategoriji"@sl ;
  skos:definition "Represents the category a dimension is grouped in."@en,
    "Predstavlja kategorijo, v katero spada dimenzija."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain dqv:Dimension ;
  rdfs:range dqv:Category .

duv:hasUsageTool
  a owl:ObjectProperty ;
  rdfs:label "has usage tool"@en,
    "ima orodje uporabe"@sl ;
  skos:definition "A usage tool (application, service) referred to by usage guidance/instructions."@en,
    "Orodje uporabe (aplikacija, storitev), ki so je referencira v smernicah/navodilih."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  rdfs:domain duv:Usage ;
  rdfs:range duv:UsageTool .

csvw:column
  a owl:ObjectProperty ;
  rdfs:label "column"@en,
    "stolpec"@sl ;
  skos:definition "An array property of column descriptions as described in section 5.6 Columns."@en,
    "Lastnost v obliki tabele, ki vsebuje opise stolpcev, kot je opisano v razdelku 5.6 Columns."@sl ;
  vaem:rationale "Mandatory."@en,
    "Obvezno."@sl ;
  owl:minCardinality 1 ;
  rdfs:domain csvw:Schema ;
  rdfs:range csvw:Column .

csvw:primaryKey
  a owl:ObjectProperty ;
  rdfs:label "primary key"@en,
    "primarni ključ"@sl ;
  skos:definition "A column reference property that holds either a single reference to a column description object or an array of references."@en,
    "Referenčna lastnost stolpca, ki je lahko ena referenca ali seznam referenc."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  owl:maxCardinality 1 ;
  rdfs:domain csvw:Schema ;
  rdfs:range xsd:string .

csvw:datatype
  a owl:ObjectProperty ;
  rdfs:label "data type"@en,
    "podatkovni tip"@sl ;
  skos:definition "An object property that contains either a single string that is the main datatype of the values of the cell or a datatype description object. If the value of this property is a string, it MUST be one of the built-in datatypes defined in section 5.11.1 Built-in Datatypes or an absolute URL; if it is an object then it describes a more specialised datatype."@en,
    "Objektna lastnost, ki vsebuje en niz, ki je glavni podatkovni tip vrednosti celic ali opisni objekt podatkovnih tipov. Če je vrednost tega tipa niz, mora biti eden izmed vgrajenih podatkovnih tipov iz razdelka 5.11.1 Built-in Datatypes or an absolute URL.Če je objekt, potem opisuje bolj specifičen podatkovni tip."@sl ;
  vaem:rationale "Recommended (Schema), Mandatory (Column)."@en,
    "Priporočeno (Shema), Obvezno (Stolpec)."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range csvw:Datatype, xsd:string .

csvw:required
  a owl:ObjectProperty ;
  rdfs:label "required"@en,
    "obvezen"@sl ;
  skos:definition "A boolean atomic property taking a single value which indicates whether the cell must have a non-null value. The default is `false`."@en,
    "Atomarna lastnost, ki sprejme eno vrednost in definira, če ima lahko celica prazno vrednost. Privzeta vrednost je `false`."@sl ;
  vaem:rationale "Recommended (Schema), Mandatory (Column)."@en,
    "Priporočeno (Shema), Obvezno (Stolpec)."@sl ;
  owl:maxCardinality 1 ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range xsd:boolean .

csvw:separator
  a owl:ObjectProperty ;
  rdfs:label "separator"@en,
    "ločnica"@sl ;
  skos:definition "An atomic property that MUST have a single string value that is the character used to separate items in the string value of the cell."@en,
    "Atomarna lastnost, ki mora biti en niz, t.j. znak, ki je uporabljen za ločevanje elementov v znakovni predstavitvi celice."@sl ;
  vaem:rationale "Recommended (Schema), Optional (Column)."@en,
    "Priporočeno (Shema), Opcijsko (Stolpec)."@sl ;
  owl:maxCardinality 1 ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range xsd:string .

csvw:aboutUrl
  a owl:ObjectProperty ;
  rdfs:label "about URL"@en,
    "naslov z opisom"@sl ;
  skos:definition "A URI template property that MAY be used to indicate what a cell contains information about."@en,
    "Predloga URI, ki je lahko uporabljena za definicijo, podatke o čem vsebuje celica."@sl ;
  vaem:rationale "Recommended."@en,
    "Priporočeno."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range csvw:uriTemplate .

csvw:rowTitle
  a owl:ObjectProperty ;
  rdfs:label "row titles"@en,
    "naslovi vrstic"@sl ;
  skos:definition "A column reference property that holds either a single reference to a column description object or an array of references."@en,
    "Referenčna lastnost stolpca, ki lahko vsebuje eno referenco na opis stolpca ali seznam referenc."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema ;
  rdfs:range xsd:string .

csvw:foreignKey
  a owl:ObjectProperty ;
  rdfs:label "foreign key"@en,
    "tuji ključ"@sl ;
  skos:definition "An array property of foreign key definitions that define how the values from specified columns within this table link to rows within this table or other tables."@en,
    "Lastnost v obliki seznama, ki vsebuje definicije tujih ključev - kako se vrednosti iz definiranih stolpcev povezujejo z drugimi tabelami."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema ;
  rdfs:range csvw:ForeignKey .

csvw:default
  a owl:ObjectProperty ;
  rdfs:label "default"@en,
    "privzeta vrednost"@sl ;
  skos:definition "An atomic property holding a single string that is used to create a default value for the cell in cases where the original string value is an empty string."@en,
    "Atomarna lastnost, ki hrani en niz, ki se uporablja kot privzeta vrednost, če je originalna vrednost prazen niz."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range xsd:string .

csvw:lang
  a owl:ObjectProperty ;
  rdfs:label "language"@en,
    "jezik"@sl ;
  skos:definition "An atomic property giving a single string language code as defined by [BCP47]."@en,
    "Atomarna lastnost, ki nizom priredi jezikovno oznako v skladu z [BCP47]."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range xsd:string .

csvw:null
  a owl:ObjectProperty ;
  rdfs:label "null"@en,
    "brez vrednosti / nič"@sl ;
  skos:definition "An atomic property giving the string or strings used for null values within the data. If the string value of the cell is equal to any one of these values, the cell value is `null`."@en,
    "Atomarna lastnost, ki izbranemu nizu ali več nizom priredi `null` vrednost, če se ujejamo z vrednostmi v tabeli."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range xsd:string .

csvw:ordered
  a owl:ObjectProperty ;
  rdfs:label "ordered"@en,
    "urejen"@sl ;
  skos:definition "A boolean atomic property taking a single value which indicates whether a list that is the value of the cell is ordered (if `true`) or unordered (if `false`)."@en,
    "Atomarna logična lastnost, ki prejme eno vrednost in vrne, če je vrednost v celici urejena ali ne."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range xsd:boolean .

csvw:propertyUrl
  a owl:ObjectProperty ;
  rdfs:label "property URL"@en,
    "URL lastnosti"@sl ;
  skos:definition "An URI template property that MAY be used to create a URI for a property if the table is mapped to another format."@en,
    "Lastnost predloge URI, ki je lahko uporabljena za kreiranje URI-ja za lastnosti, če je tabela pretvorjena v drug format."@sl ;
  vaem:rationale "Optional."@en,
    "Opcijsko."@sl ;
  rdfs:domain csvw:Schema, csvw:Column ;
  rdfs:range csvw:uriTemplate .

csvw:transformations
